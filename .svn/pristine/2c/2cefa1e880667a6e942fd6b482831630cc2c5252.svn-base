package com.wms.core.combobox;

/**
 * 供货状态
 * @author chengangxiong
 */
public enum SupplyCondition {
    /**
     * 正常供货
     */
    NORMAL_SUPPLY("normal"),
    
    /**
     * 异常供货
     */
    EXCEPTION_SUPPLY("exception");
    
    private final String condition;
    
    private SupplyCondition(String condition){
        this.condition = condition;
    }
    
    public String getCondition(){
        return condition;
    }
}
